tags:
{{#prefix}}
  - {{.}}
{{/prefix}}
summary: "{{{options.summary}}}"
operationId: {{{options.operationId}}}
{{#options.parameters.length}}
  parameters:
  {{#options.parameters}}
    - $ref: "{{{.}}}"
  {{/options.parameters}}
{{/options.parameters.length}}
{{#options.requestBody}}
  requestBody:
  content:
    {{{options.content}}}:
      schema:
        $ref: "{{{.}}}"
{{/options.requestBody}}
responses:
{{#route.responses}}
  {{statusCode}}:
    description: "{{{description}}}"
    {{#object}}
    content:
      {{{content}}}:
        schema:
          allOf:
          - $ref: "{{{refPath}}}"
  {{/object}}
{{/route.responses}}
{{#security.length}}
  security:
  {{#security}}
    - {{{.}}}{{^isSecurityOptional}}: []{{/isSecurityOptional}}
  {{/security}}
{{/security.length}}
